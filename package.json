{
  "name": "vscode-json-table-view",
  "displayName": "JSON Table View",
  "description": "View JSON as table",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tableViewUi.start",
    "onWebviewPanel:tableViewUi"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tableViewUi.start",
        "title": "Start tableViewUi",
        "category": "Table View Ui"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "dev": "tsc -watch -p ./",
    "pretest": "npm run build && npm run lint",
    "test": "node ./dist/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.47.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "handlebars": "^4.7.6"
  }
}
